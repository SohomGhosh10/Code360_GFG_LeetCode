class Solution {

    // Note that the size of the array is n-1
    int missingNumber(int n, int arr[]) {

        // Your Code Here
        int total_sum = (n*(n+1)) / 2;
        int curr_sum = 0;
        
        for(int i:arr){
            curr_sum += i;
        }
        
    return total_sum - curr_sum;
    }
}

